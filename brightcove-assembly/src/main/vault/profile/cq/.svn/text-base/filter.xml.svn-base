<?xml version="1.0" encoding="UTF-8"?>
<!-- Defines which repository items are generally included -->
<workspaceFilter version="1.0">
    <!--  remove compiled JSP classes -->
    <filter root="/var/classes/org/apache/jsp/apps/iolb" />

    <filter root="/apps">
        <include pattern="/apps/iolb(/.*)?" />
        <exclude pattern="/apps/iolb/config(.*)?" />
    </filter>

    <filter root="/etc/designs">
        <include pattern="/etc/designs/iolb(/.*)?" />
    </filter>
    <filter root="/etc/docroot">
        <include pattern="/etc/docroot/iolb(/.*)?" />
    </filter>

    <!-- OLB specific Zen Garden overrides -->
    <filter root="/apps/zg/commons/components/base/htmlInclude" />
    <filter root="/apps/zg/commons/components/base/richText/dialog_rte_plugins" />

    <filter root="/content/investec/olb" />
    <filter root="/content/dam/investec">
        <include pattern="/content/dam/investec/olb(/.*)?" />
        <exclude pattern=".*/renditions/.*" />
        <include pattern=".*/renditions/original(.*)?" />
    </filter>
</workspaceFilter>
